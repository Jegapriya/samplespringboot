node{
   stage('SCM Checkout'){
     git 'https://github.com/PraksahAbd/samplespringboot.git'
   }
   stage('Compile-Package'){
      // Get maven home path
      def mvnHome =  tool name: 'MavenPrjct', type: 'maven'   
      sh "${mvnHome}/bin/mvn package"
   }
   
   stage('SonarQube Analysis') {
        def mvnHome =  tool name: 'MavenPrjct', type: 'maven'
        withSonarQubeEnv('SonarQube') { 
          sh 'mvn sonar:sonar \
          -Dsonar.host.url=http://jekai38211dns.eastus.cloudapp.azure.com \
          -Dsonar.login=795e7bd8dcdeff3c705fb8657c918017c30d5bb7'
        }
    }
   

}
